pipeline {
    agent any
    
    stages {
        
        
        stage('GIT Checkout') {
            steps {
                script {
                    echo " Getting Project from Git "
		    
                }
            }
        }
        
        stage('MVN CLEAN') {
            steps {
                script {
                    
                     sh '.............'
                    
                }
            }
        }
        
        stage('MVN COMPILE') {
            steps {
                script {   
                      sh '......'
                    
                }
            }
        }
        
        stage('MVN SONARQUBE') {
            steps {
                script {
                   
                       sh '.......................'
                    
                }
            }
        }
        
        stage('MVN TEST') {
            steps {
                script {
                    
                       sh 'mvn test'
                    
                }
            }
        }
    }
}